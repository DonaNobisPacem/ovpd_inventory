<%= simple_form_for(@transaction_item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :transaction %>
    <%= f.input :quantity %>
    <%= f.input :product_id %>
    <%= f.input :transaction_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
